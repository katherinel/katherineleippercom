= form_for @image, html: { multipart: true } do |f|
	- if @image.errors.any?
		#error_explanation= "#{pluralize(@image.errors.count, "error")} prohibited this image from being saved:"
		%ul
			- @image.errors.full_messages.each do |message|
				%li= message

	= f.label :image
	= f.file_field :image
	.action
		= f.submit